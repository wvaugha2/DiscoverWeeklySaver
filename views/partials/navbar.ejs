<nav class="navbar navbar-expand-lg navbar-dark bgPrimary absoluteTop">
  <div class="container">
    <div class="navbar-brand">Discover Weekly Saver</div>
    <ul class="navbar-nav ml-auto">
      <!-- Render all Nav Links -->
      <% if(Array.isArray(links)) { %>
        <% links.forEach((link) => { %>
          <li class="nav-item">
            <a class="nav-link active"
              <% if(link.disabled) { %> disabled <% } %>
              href="<%- link.url %>"
            ><%= link.text %></a>
          </li>
        <% }) %>
      <% } %>
    </ul>
  </div>
</nav>